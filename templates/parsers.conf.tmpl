{% for parser in parsers %}
[PARSER]
    {%- for line in parser %}
    {% for item in line%}{{ item }} {% endfor %}
    {%- endfor %}
{%- endfor %}

{% for parser in multiline_parsers %}
[MULTILINE_PARSER]
    {%- for line in parser %}
    {% for item in line%}{{ item }} {% endfor %}
    {%- endfor %}
{%- endfor %}
